<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netcoreapp3.1;netcoreapp2.1;net48;net472;net462</TargetFrameworks>
    <AssemblyName>Model.Fingerprint</AssemblyName>
    <RootNamespace>Fingerprint</RootNamespace>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageId>Model.Fingerprint</PackageId>
    <Authors>gonace,laget.se</Authors>
    <Company>laget.se</Company>
    <PackageProjectUrl>https://github.com/laget-se/FingerprintBuilder</PackageProjectUrl>
    <Icon>https://raw.githubusercontent.com/laget-se/FingerprintBuilder/master/assets/icon.png</Icon>
    <Product>Model.Fingerprint</Product>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryUrl>https://github.com/laget-se/FingerprintBuilder</RepositoryUrl>
    <PackageIcon>icon.png</PackageIcon>
    <PackageIconUrl />
    <RepositoryType>git</RepositoryType>
    <Description>Calculates a fingerprint (hash) for an object that can be stored in Memory or a persistent data store.</Description>
    <PackageTags>hash;fingerprint</PackageTags>
    <AssemblyVersion>1.0.0.1</AssemblyVersion>
    <FileVersion>1.0.0.1</FileVersion>
    <Version>1.0.0.1</Version>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\assets\icon.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
